{
    "name": "hotash/laravel-multi-ui",
    "description": "Laravel Ui Package For Multiple Guards.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Sumon Ahmed",
            "email": "bdsumon4u@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2.5",
        "illuminate/console": "^7.0",
        "illuminate/filesystem": "^7.0",
        "illuminate/support": "^7.0"
